<?xml version="1.0" encoding="UTF-8"?><sqlb_project><db path="fleet.db" readonly="0" foreign_keys="1" case_sensitive_like="0" temp_store="0" wal_autocheckpoint="1000" synchronous="2"/><attached/><window><main_tabs open="structure browser pragmas query" current="3"/></window><tab_structure><column_width id="0" width="300"/><column_width id="1" width="0"/><column_width id="2" width="100"/><column_width id="3" width="992"/><column_width id="4" width="0"/><expanded_item id="0" parent="1"/><expanded_item id="1" parent="1"/><expanded_item id="2" parent="1"/><expanded_item id="3" parent="1"/></tab_structure><tab_browse><table title="ota_logs" custom_title="0" dock_id="1" table="4,8:mainota_logs"/><dock_state state="000000ff00000000fd00000001000000020000000000000000fc0100000001fb000000160064006f0063006b00420072006f00770073006500310100000000ffffffff0000011300ffffff000000000000000000000004000000040000000800000008fc00000000"/><default_encoding codec=""/><browse_table_settings/></tab_browse><tab_sql><sql name="SQL 1*">WITH RegionalStats AS (
    SELECT 
        region,
        COUNT(*) as total_devices,
        SUM(CASE WHEN update_status = 'Partial' THEN 1 ELSE 0 END) as failed_count
    FROM ota_logs
    GROUP BY region
)
SELECT 
    region,
    total_devices,
    failed_count,
    ROUND((failed_count * 100.0 / total_devices), 2) as failure_rate_percentage
FROM RegionalStats
ORDER BY failure_rate_percentage DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
